<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".BeneficiarioSocioEconomico">

    <android.support.design.widget.TextInputLayout

        android:gravity="bottom"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="parent"
        android:id="@+id/profile_input_layout_name"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textColorHint="#9e000000"
        app:hintTextAppearance="@style/login_hintTextAppearance"
        android:layout_marginStart="24dp"
        app:hintEnabled="true"
        android:layout_marginEnd="4dp"
        app:layout_constraintEnd_toStartOf="@+id/profile_input_layout_lastname"
        app:hintAnimationEnabled="true"
        app:layout_constraintStart_toStartOf="parent">

        <android.support.design.widget.TextInputEditText
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textSize="15sp"
            android:enabled="false"
            android:focusable="false"
            android:fontFamily="sans-serif"
            android:text="Block y adobe"
            android:textColor="@color/colorPrimaryDark"
            android:theme="@style/login_editControl"
            android:hint="Material de vivienda" />
    </android.support.design.widget.TextInputLayout>

    <android.support.design.widget.TextInputLayout
        android:gravity="bottom"
        android:layout_marginEnd="24dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/profile_input_layout_name"
        app:layout_constraintEnd_toEndOf="parent"
        android:id="@+id/profile_input_layout_lastname"
        android:layout_width="0dp"

        android:layout_height="wrap_content"
        android:textColorHint="#9e000000"
        app:hintTextAppearance="@style/login_hintTextAppearance"
        app:hintEnabled="true"
        android:focusable="false"
        app:hintAnimationEnabled="true" >

        <android.support.design.widget.TextInputEditText
            android:layout_width="match_parent"
            android:textColor="@color/colorPrimaryDark"
            android:layout_height="match_parent"
            android:textSize="15sp"
            android:enabled="false"
            android:focusable="false"
            android:fontFamily="sans-serif"
            android:text="3"
            android:theme="@style/login_editControl"
            android:hint="Cuartos" />

    </android.support.design.widget.TextInputLayout>


    <android.support.design.widget.TextInputLayout

        android:gravity="bottom"
        android:layout_marginStart="24dp"
        android:layout_marginTop="4dp"
        app:layout_constraintTop_toBottomOf="@id/profile_input_layout_name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/profile_input_dia_nacimiento"
        android:id="@+id/profile_input_edad"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textColorHint="#9e000000"
        app:hintTextAppearance="@style/login_hintTextAppearance"
        app:hintEnabled="true"
        android:focusable="false"
        app:hintAnimationEnabled="true" >

        <android.support.design.widget.TextInputEditText
            android:layout_width="match_parent"
            android:textColor="@color/colorPrimaryDark"
            android:layout_height="match_parent"
            android:textSize="15sp"
            android:enabled="false"
            android:focusable="false"
            android:fontFamily="sans-serif"
            android:text="Desempleado"
            android:theme="@style/login_editControl"
            android:hint="Ocupacion" />

    </android.support.design.widget.TextInputLayout>

    <android.support.design.widget.TextInputLayout

        android:gravity="bottom"
        android:layout_marginEnd="24dp"
        android:layout_marginTop="4dp"
        app:layout_constraintTop_toBottomOf="@id/profile_input_layout_name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/profile_input_edad"
        android:id="@+id/profile_input_dia_nacimiento"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textColorHint="#9e000000"
        app:hintTextAppearance="@style/login_hintTextAppearance"
        app:hintEnabled="true"
        android:focusable="false"
        app:hintAnimationEnabled="true" >

        <android.support.design.widget.TextInputEditText
            android:layout_width="match_parent"
            android:textColor="@color/colorPrimaryDark"
            android:layout_height="match_parent"
            android:textSize="15sp"
            android:enabled="false"
            android:focusable="false"
            android:fontFamily="sans-serif"
            android:text="Sin estudios"
            android:theme="@style/login_editControl"
            android:hint="Grado de estudios" />

    </android.support.design.widget.TextInputLayout>

    <android.support.design.widget.TextInputLayout

        android:gravity="bottom"
        android:layout_marginStart="24dp"
        android:layout_marginTop="4dp"
        app:layout_constraintTop_toBottomOf="@id/profile_input_edad"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/profile_input_etnia"
        android:id="@+id/profile_input_telefono"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textColorHint="#9e000000"
        app:hintTextAppearance="@style/login_hintTextAppearance"
        app:hintEnabled="true"
        android:focusable="false"
        app:hintAnimationEnabled="true" >

        <android.support.design.widget.TextInputEditText
            android:layout_width="match_parent"
            android:textColor="@color/colorPrimaryDark"
            android:layout_height="match_parent"
            android:textSize="15sp"
            android:enabled="false"
            android:focusable="false"
            android:fontFamily="sans-serif"
            android:text="9"
            android:theme="@style/login_editControl"
            android:hint="Nº de hijos" />

    </android.support.design.widget.TextInputLayout>

    <android.support.design.widget.TextInputLayout

        android:gravity="bottom"
        android:layout_marginEnd="24dp"
        android:layout_marginTop="4dp"
        app:layout_constraintTop_toBottomOf="@id/profile_input_dia_nacimiento"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/profile_input_telefono"
        android:id="@+id/profile_input_etnia"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textColorHint="#9e000000"
        app:hintTextAppearance="@style/login_hintTextAppearance"
        app:hintEnabled="true"
        android:focusable="false"
        app:hintAnimationEnabled="true" >

        <android.support.design.widget.TextInputEditText
            android:layout_width="match_parent"
            android:textColor="@color/colorPrimaryDark"
            android:layout_height="match_parent"
            android:textSize="15sp"
            android:enabled="false"
            android:focusable="false"
            android:fontFamily="sans-serif"
            android:text="1"
            android:theme="@style/login_editControl"
            android:hint="Nº Dependientes" />

    </android.support.design.widget.TextInputLayout>

    <android.support.design.widget.TextInputLayout

        android:gravity="bottom"
        android:layout_marginStart="24dp"
        android:layout_marginTop="4dp"
        app:layout_constraintTop_toBottomOf="@id/profile_input_telefono"
        app:layout_constraintStart_toStartOf="parent"
        android:id="@+id/profile_input_sexo"
        android:layout_width="0dp"
        app:layout_constraintEnd_toStartOf="@id/profile_input_estado_civil"
        android:layout_height="wrap_content"
        android:textColorHint="#9e000000"
        app:hintTextAppearance="@style/login_hintTextAppearance"
        app:hintEnabled="true"
        android:focusable="false"
        app:hintAnimationEnabled="true" >

        <android.support.design.widget.TextInputEditText
            android:layout_width="match_parent"
            android:textColor="@color/colorPrimaryDark"
            android:layout_height="match_parent"
            android:textSize="15sp"
            android:enabled="false"
            android:focusable="false"
            android:fontFamily="sans-serif"
            android:text="$2,200.00"
            android:theme="@style/login_editControl"
            android:hint="Ingreso mensual"/>

    </android.support.design.widget.TextInputLayout>

    <android.support.design.widget.TextInputLayout

        android:gravity="bottom"
        android:layout_marginEnd="24dp"
        android:layout_marginTop="4dp"
        app:layout_constraintTop_toBottomOf="@id/profile_input_etnia"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/profile_input_sexo"
        android:id="@+id/profile_input_estado_civil"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textColorHint="#9e000000"
        app:hintTextAppearance="@style/login_hintTextAppearance"
        app:hintEnabled="true"
        android:focusable="false"
        app:hintAnimationEnabled="true" >

        <android.support.design.widget.TextInputEditText
            android:layout_width="match_parent"
            android:textColor="@color/colorPrimaryDark"
            android:layout_height="match_parent"
            android:textSize="15sp"
            android:enabled="false"
            android:focusable="false"
            android:fontFamily="sans-serif"
            android:text="Pension"
            android:theme="@style/login_editControl"
            android:hint="Medio" />

    </android.support.design.widget.TextInputLayout>


</android.support.constraint.ConstraintLayout>